{
  "name": "wp-node",
  "version": "0.1.0",
  "description": "An offline-first SPA using Vue.js, the WordPress REST API and Progressive Web Apps",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bstavroulakis/vue-wordpress-pwa"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "keywords": [
    "vue",
    "vuex",
    "offline",
    "progressive-web-app",
    "pwa",
    "webpack",
    "starter",
    "server-side",
    "boilerplate"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=production node server",
    "dev": "rimraf ./dist && cross-env NODE_ENV=development nodemon -L server.js",
    "build": "node build/build.js",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.config.js --progress --hide-modules"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bulma": "^0.4.0",
    "clean-html": "^1.4.3",
    "compression": "^1.6.2",
    "cross-env": "^3.2.4",
    "es6-promise": "^4.1.0",
    "express": "^4.15.2",
    "localforage": "^1.5.0",
    "lru-cache": "^4.0.2",
    "postcss-discard-comments": "^2.0.4",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.4.1",
    "vue": "^2.3.2",
    "vue-resource": "^1.3.4",
    "vue-router": "^2.5.0",
    "vue-server-renderer": "^2.3.2",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.2.2",
    "eslint-config-standard": "^6.0.0-beta.2",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "fs-extra": "^2.1.2",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "md5": "^2.2.1",
    "mocha": "^3.0.2",
    "nodemon": "^1.11.0",
    "postcss": "^6.0.3",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "purifycss-webpack": "^0.5.0",
    "recursive-readdir": "^2.1.1",
    "rimraf": "^2.6.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "vue-loader": "^12.0.2",
    "vue-style-loader": "^3.0.0",
    "vue-template-compiler": "^2.3.2",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  }
}
